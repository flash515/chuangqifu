<view>
  <swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}"
    vertical="{{vertical}}" interval="{{interval}}" duration="{{duration}}" previous-margin="{{previousMargin}}px"
    next-margin="{{nextMargin}}px">
    <block wx:for="{{image}}" wx:for-index="index" wx:key="key">
      <swiper-item>
        <image src="{{item}}" class="swiper image" mode="scaleToFill" />
        <!-- <view class="swiper-item {{item.url}}"></view> -->
      </swiper-item>
    </block>
  </swiper>
</view>

<l-tabs bind:linchange="changeTabs">
  <l-tabpanel tab="自然人代开" key="one" slot="one">
    <view class="tab-content">
      <view>
        <view wx:for="{{DKarray}}" wx:key="key">
          <l-card type="primary" plaintext="{{true}}">
            <view class="content">
              <view class="title1">
                {{item.ProductName}}
              </view>
              <label class="title1">编号{{item.ProductId}}</label>
              <view>
                {{item.Outline}}
              </view>
              <label class="title1">办理费用：</label>
              <view wx:if="{{avatarUrl=='' || nickName==''}}">
                <button class="userinfo-nickname" bindtap='getUserProfile'>微信登录</button>
              </view>
              <view wx:else>
                <view wx:if="{{usertype == 'admin'||usertype == 'provider'||pricelevel == 'PL1'}}"> {{item.Price1}}
                </view>
                <view wx:if="{{usertype == 'admin'||usertype == 'provider'||pricelevel == 'PL2'}}"> {{item.Price2}}
                </view>
                <view wx:if="{{usertype == 'admin'||usertype == 'provider'||pricelevel == 'PL3'}}"> {{item.Price3}}
                </view>
                <view wx:if="{{usertype == 'admin'||usertype == 'provider'||pricelevel == 'PL4'}}"> {{item.Price4}}
                </view>
              </view>
              <label class="title1">推荐程度：</label>
              <l-rate active-color="red" inActive-color="grey" score="{{item.Score}}" count="10" />
              <view>
                <l-button data-id="_id={{item._id}}" bind:lintap="bvPersonalDetail" plain="{{true}}">详细介绍</l-button>
                <text>\t\t</text>
                <l-button
                  data-id="productid={{item.ProductId}}&productname={{item.ProductName}}&issuedplace={{item.IssuedPlace}}"
                  bind:lintap="bvNewPersonal">我要办理</l-button>
              </view>
            </view>
          </l-card>
        </view>
      </view>
    </view>
  </l-tabpanel>
